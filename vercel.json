{
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/categories",
        "dest": "index.js"
      },
      {
        "src": "/categories/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brands",
        "dest": "index.js"
      },
      {
        "src": "/brands/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/users/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/cart",
        "dest": "server.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/team",
        "dest": "server.js"
      },
      {
        "src": "/api/team(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/user/fetchAllUser(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/user/adduser(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/user(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/query(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/query",
        "dest": "server.js"
      },
      {
        "src": "/api/user/update/:id",
        "dest": "server.js"
      },
      {
        "src": "/api/user/adduser",
        "dest": "server.js"
      },
      {
        "src": "/api/user/fetchAllUser",
        "dest": "server.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }
  